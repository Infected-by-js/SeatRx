{
  "version": "0.1.0",
  "name": "seats_sync_backend",
  "description": "Backend for the SeatsSync project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src -e ts --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "module-alias": "^2.2.3",
    "nanoid": "^5.0.8",
    "node-cache": "^5.1.2",
    "nodemon": "^3.1.7",
